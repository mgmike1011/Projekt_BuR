{
    "name": "node-red-project",
    "version": "0.0.1",
    "description": "A Node-RED Project",
    "main": "main.js",
    "NRelectron": {
        "productName": "Test",
        "editable": true,
        "allowLoadSave": false,
        "showMap": false,
        "kioskMode": false,
        "addNodes": true,
        "flowFile": "flows.json",
        "start": "dashboard",
        "showmap": false
    },
    "scripts": {
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "release": "electron-builder build -p always",
        "postinstall": "electron-builder install-app-deps",
        "start": "electron main.js",
        "test": "echo \" Warning: no test specified \""
    },
    "dependencies": {
        "electron-is-dev": "^1.2.0",
        "electron-store": "^6.0.0",
        "express": "^4.17.1",
        "node-red": "3.0.2",
        "node-red-contrib-aedes": "0.8.2",
        "node-red-dashboard": "3.1.7",
        "node-red-node-rbe": "~0.2.9",
        "node-red-node-ui-table": "~0.3.12"
    },
    "keywords": [
        "electron",
        "quick start",
        "node-red"
    ],
    "devDependencies": {
        "electron": "~10.1.3",
        "electron-builder": "^22.9.1"
    },
    "build": {
        "appId": "com.electron.node-red-app",
        "productName": "Test",
        "copyright": "Copyright Â© 2018,2020 D.Conway-Jones",
        "buildDependenciesFromSource": true,
        "files": [
            "**/*",
            {
                "from": ".node_modules",
                "to": ".node_modules"
            }
        ],
        "mac": {
            "category": "public.app-category.developer-tools",
            "target": "dmg"
        },
        "linux": {
            "target": [
                "rpm",
                {
                    "target": "deb",
                    "arch": [
                        "x64",
                        "armv7l"
                    ]
                }
            ],
            "synopsis": "Node-RED Low Code development",
            "category": "Development"
        },
        "win": {
            "target": [
                {
                    "target": "msi",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        }
    }
}
